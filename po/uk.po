msgstr "Повністю перемалювати"

#: ../src/ui/dialog/inkscape-preferences.cpp:2868
msgid "Multiscale"
msgstr "Мультимасштаб"

#: ../src/ui/dialog/inkscape-preferences.cpp:2870
msgid "Update strategy:"
msgstr "Стратегія оновлення:"

#: ../src/ui/dialog/inkscape-preferences.cpp:2870
